{{>layouts/header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">Title</th>
        <th scope="col">Contents</th>
        <th scope="col">Writer</th>
    </tr>
    </thead>
    <tbody>
    {{#articleList}}
        <tr>
            <td><a href="/articles/{{id}}">{{title}}</a></td>
            <td>{{contents}}</td>
            <td>{{userNickname}}</td>
        </tr>
    {{/articleList}}
    </tbody>
</table>
<a href="/articles/new">New Article</a>

{{>layouts/footer}}